<h1>Clicking <%= @user.name %></h1>

<%= render :partial => "flash", :locals => { :flash => flash } %>

<%= form_tag(@next_user ? click_url(:id => @next_user.id) : {} ,:method => :get) do %>
  <%= submit_tag 'Next User', :disabled => !@next_user %>
  <%= submit_tag 'Skip User', :disabled => !@next_user %>
<% end %>

<%= button_to "End Clicking", root_path, :method => :get %>
Currently clicking as: 	<%= @selected_user = session[:current_user];
	@selected_user.name %>

<iframe src=<%= @user.url %> width="100%" height="600">
  <p>Your browser does not support iframes.</p>
</iframe>