<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
  <%= favicon_link_tag %>
</head>
<body>

<div id="main">

	<%= render :partial => 'shared/top_nav' %>
	<br />
	<p><div id="header">
		<%= yield :header %>
	</div></p>
	<br />
	<%= render :partial => 'shared/flash', :locals => { :flash => flash } %>
	<br />
	
	<div id="content-pane">
	<%= yield %>
	</div>
	<br />
	<%= render :partial => 'shared/footer'%>
	
</div>

</body>
</html>
