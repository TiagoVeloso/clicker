<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>

</head>
<body>
<body style="background: #EEEEEE;">

<%= debug session %>

<% if logged_in? %>
<%= link_to "Log Out", 'logout' %>
<% end %>
<% if !logged_in? %>
<%= link_to "Log In", 'login' %>
<% end %>

<%= yield %>

</body>
</html>
